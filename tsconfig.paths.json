      "@api": ["./src/api"],
      "@api/*": ["./src/api/*"],
      "@pages": ["./src/pages"],
      "@pages/*": ["./src/pages/*"],
      "@layouts": ["./src/layouts"],
      "@layouts/*": ["./src/layouts/*"],
